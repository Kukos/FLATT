VAR

a b c

BEGIN

READ a;
READ b;

c := a / b;
a := b / c;
b := a / c;

WRITE a;
WRITE b;
WRITE c;

READ a;
b := 3000;

c := a / b;
a := b / c;
b := a / c;

WRITE a;
WRITE b;
WRITE c;

a := 10000;
READ b;

c := a / b;
a := b / c;
b := a / c;

WRITE a;
WRITE b;
WRITE c;

READ a;
a := 10000 / a;
WRITE a;

END
