VAR

a b c


BEGIN

a := 10;

FOR i FROM 1 DOWNTO a DO
    WRITE i;
ENDFOR

b := 1;

FOR i FROM b DOWNTO 10 DO
    WRITE i;
ENDFOR

FOR i FROM b DOWNTO a DO
    WRITE i;
ENDFOR

READ a;

FOR i FROM 1 DOWNTO a DO
    WRITE i;
ENDFOR

READ b;

FOR i FROM b DOWNTO 10 DO
    WRITE i;
ENDFOR

FOR i FROM b DOWNTO a DO
    WRITE i;
ENDFOR

END
