 n  :=  1000  -  1 
FOR  i  FROM  2  TO  n 
 key  :=  t[ i ] 
 j  :=  i  -  1 
 l  :=  j 
WHILE	 j  >  0 
IF	 t[ j ]  >  key 
 k  :=  j  +  1 
 t[ k ]  :=  t[ j ] 
 l  :=  j 
 j  :=  j  -  1 
ELSE
 j  :=  j  -  1 
ENDIF
ENDWHILE
 t[ l ]  :=  key 
ENDFOR
FOR  i  FROM  0  TO  n 
WRITE	 t[ i ] 
ENDFOR
